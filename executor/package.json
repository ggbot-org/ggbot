{
  "name": "@workspace/executor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "tsc --build --clean tsconfig.build.json",
    "e2e:execute": "node $npm_package_config_tsnode ./src/end-to-end-tests/execute.ts",
    "e2e:listAccountKeys": "node $npm_package_config_tsnode ./src/end-to-end-tests/listAccountKeys.ts",
    "lint": "eslint --fix --ext .ts src/",
    "prebuild": "npm run cleanup",
    "start": "node dist/start"
  },
  "devDependencies": {
    "@workspace/eslint-config": "0.0.0",
    "@workspace/tsconfig": "0.0.0"
  },
  "dependencies": {
    "@workspace/api": "0.0.0",
    "@workspace/authentication": "0.0.0",
    "@workspace/binance": "0.0.0",
    "@workspace/cache": "0.0.0",
    "@workspace/database": "0.0.0",
    "@workspace/dflow": "0.0.0",
    "@workspace/env": "0.0.0",
    "@workspace/locators": "0.0.0",
    "@workspace/models": "0.0.0",
    "@workspace/s3-data-bucket": "0.0.0",
    "flow-view": "6.0.1",
    "minimal-time-helpers": "0.1.0",
    "read-file-utf8": "3.0.0",
    "write-file-utf8": "3.0.2"
  },
  "config": {
    "tsnode": "--no-warnings=ExperimentalWarning --loader ts-node/esm"
  }
}
