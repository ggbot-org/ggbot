{
  "name": "@workspace/executor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/",
    "pretest": "tsc --build tsconfig.test.json",
    "start": "node dist/start",
    "test": "node --test"
  },
  "devDependencies": {
    "@workspace/eslint-config": "file:eslint-config",
    "@workspace/tsconfig": "file:tsconfig"
  },
  "dependencies": {
    "@workspace/cache": "file:cache",
    "@workspace/database": "file:database",
    "@workspace/env": "file:env",
    "@workspace/models": "file:models",
    "flow-view": "6.0.1",
    "minimal-time-helpers": "0.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@workspace/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "no-warning-comments": "off"
        }
      }
    ]
  }
}
