{
  "name": "@workspace/aws-ses",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "cleanup": "tsc --build --clean tsconfig.build.json",
    "check_types": "tsc --noEmit",
    "e2e:sendEmail": "node $npm_package_config_tsnode ./src/end-to-end-tests/sendEmail.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "3.632.0",
    "@workspace/aws-types": "0.0.0"
  },
  "devDependencies": {
    "@workspace/tsconfig": "0.0.0"
  },
  "config": {
    "tsnode": "--no-warnings=ExperimentalWarning --loader ts-node/esm"
  }
}
