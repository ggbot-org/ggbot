{
  "name": "@ggbot2/aws-lambda-auth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/index.js",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ build.zip",
    "lint": "eslint src/**/*.ts",
    "prebuild": "npm run cleanup",
    "prezip": "npm run build",
    "zip": "cd dist; zip -X -r ../build.zip * > /dev/null; cd .."
  },
  "dependencies": {
    "@ggbot2/http-status-codes": "file:http-status-codes",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/tsconfig": "file:tsconfig",
    "@types/aws-lambda": "^8.10.114",
    "esbuild": "^0.17.17"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ]
  }
}
