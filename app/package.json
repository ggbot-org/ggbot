{
  "name": "@ggbot2/app",
  "description": "user webapp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run generate_public_files",
    "build_translations": "npm run formatjs:compile:en",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "formatjs:compile:en": "formatjs compile translations/en.json --ast --out-file public/translations/en.json",
    "generate_css": "npm run sass",
    "generate_formatjs_types": "node src/scripts/generate_formatjs_types.js",
    "generate_html": "node temp/scripts/generate_html.js",
    "generate_js": "node temp/scripts/generate_js.js",
    "generate_public_files": "npm run build_translations && npm run generate_html && npm run generate_js && npm run generate_css",
    "lint": "eslint --fix --ext .ts --ext .tsx .",
    "postbuild_translations": "npm run generate_formatjs_types",
    "pregenerate_public_files": "npm run tsc:scripts",
    "prestart": "npm run generate_public_files",
    "sass": "sass --style=compressed --load-path=../node_modules/ --no-source-map src/styles/main.scss public/main.css",
    "start": "esbuild --servedir=public",
    "tsc:scripts": "tsc --build tsconfig.scripts.json"
  },
  "dependencies": {
    "@ggbot2/assets": "file:assets",
    "@ggbot2/binance": "file:binance",
    "@ggbot2/design": "file:design",
    "@ggbot2/dflow": "file:dflow",
    "@ggbot2/env": "file:env",
    "@ggbot2/i18n": "file:i18n",
    "@ggbot2/locators": "file:locators",
    "@ggbot2/models": "file:models",
    "@ggbot2/time": "file:time",
    "@ggbot2/type-utils": "file:type-utils",
    "@ggbot2/use-action": "file:use-action",
    "country-isocode2": "^1.1.0",
    "react-hot-toast": "^2.3.0"
  },
  "devDependencies": {
    "@ggbot2/esbuild": "file:esbuild",
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/html": "file:html",
    "@ggbot2/repo": "file:repo",
    "@ggbot2/tsconfig": "file:tsconfig"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config/react"
    ]
  }
}
