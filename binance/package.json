{
  "name": "@workspace/binance",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/",
    "pretest": "tsc --build tsconfig.test.json",
    "test": "node --test"
  },
  "devDependencies": {
    "@workspace/eslint-config": "file:eslint-config",
    "@workspace/tsconfig": "file:tsconfig"
  },
  "dependencies": {
    "@workspace/arithmetic": "file:arithmetic",
    "@workspace/cache": "file:cache",
    "@workspace/http": "file:http"
  },
  "eslintConfig": {
    "extends": [
      "@workspace/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "no-warning-comments": "off"
        }
      }
    ]
  }
}
