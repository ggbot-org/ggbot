{
  "name": "@workspace/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/",
    "ts-prune": "ts-prune --error --ignore 'dist|temp|src/index\\.ts'"
  },
  "dependencies": {
    "@workspace/aws": "file:aws",
    "@workspace/binance": "file:binance",
    "@workspace/binance-client": "file:binance-client",
    "@workspace/dflow": "file:dflow",
    "@workspace/email-messages": "file:email-messages",
    "@workspace/env": "file:env",
    "@workspace/infrastructure": "file:infrastructure",
    "@workspace/locators": "file:dflow",
    "@workspace/test-data": "file:test-data",
    "minimal-type-guard-helpers": "0.1.1"
  },
  "devDependencies": {
    "@workspace/eslint-config": "file:eslint-config",
    "@workspace/tsconfig": "file:tsconfig",
    "ts-prune": "0.10.3"
  },
  "eslintConfig": {
    "extends": [
      "@workspace/eslint-config"
    ]
  }
}
