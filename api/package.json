{
  "name": "@workspace/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/"
  },
  "dependencies": {
    "@workspace/database": "file:database",
    "@workspace/http": "file:http",
    "@workspace/models": "file:models",
    "@workspace/type-utils": "file:type-utils",
    "dflow": "0.42.0"
  },
  "devDependencies": {
    "@workspace/eslint-config": "file:eslint-config",
    "@workspace/tsconfig": "file:tsconfig"
  },
  "eslintConfig": {
    "extends": [
      "@workspace/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "no-warning-comments": "off"
        }
      }
    ]
  }
}
