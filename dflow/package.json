{
  "name": "@workspace/dflow",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/",
    "pretest": "tsc --build tsconfig.test.json",
    "test": "node --test",
    "ts-prune": "ts-prune --error --ignore 'dist|temp|src/index\\.ts'"
  },
  "dependencies": {
    "@workspace/binance": "0.0.0",
    "flow-view": "6.0.1",
    "minimal-type-guard-helpers": "0.1.1"
  },
  "devDependencies": {
    "@workspace/eslint-config": "0.0.0",
    "@workspace/tsconfig": "0.0.0",
    "ts-prune": "0.10.3"
  },
  "eslintConfig": {
    "extends": [
      "@workspace/eslint-config"
    ]
  }
}
