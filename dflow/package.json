{
  "name": "@workspace/dflow",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "cleanup": "tsc --build --clean tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "lint": "eslint --fix --ext .ts src/",
    "prebuild": "npm run cleanup",
    "test": "node --test $npm_package_config_tsnode `find src/ -name \"*_test.ts\"`"
  },
  "dependencies": {
    "@workspace/arithmetic": "0.0.0",
    "@workspace/binance": "0.0.0",
    "@workspace/models": "0.0.0",
    "dflow": "0.43.0",
    "flow-view": "6.0.1",
    "minimal-type-guard-helpers": "0.2.0"
  },
  "devDependencies": {
    "@workspace/eslint-config": "0.0.0",
    "@workspace/tsconfig": "0.0.0",
    "minimal-assertion-helpers": "0.1.0"
  },
  "config": {
    "tsnode": "--no-warnings=ExperimentalWarning --loader ts-node/esm"
  }
}
