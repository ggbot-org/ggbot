{
  "name": "@ggbot2/infrastructure",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --ext .ts .",
    "check_types": "tsc --noEmit --project ."
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/locators": "file:locators",
    "@ggbot2/tsconfig": "file:tsconfig"
  },
  "dependencies": {
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.298.0",
    "@aws-sdk/client-s3": "^3.298.0",
    "@ggbot2/env": "file:env"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ],
    "ignorePatterns": [
      "dist/",
      "temp/"
    ]
  }
}
