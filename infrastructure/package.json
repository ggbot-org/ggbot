{
  "name": "@workspace/infrastructure",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "3.379.1",
    "@aws-sdk/client-elastic-load-balancing-v2": "3.379.1",
    "@aws-sdk/client-iam": "3.379.1",
    "@aws-sdk/client-s3": "3.379.1",
    "@aws-sdk/client-ses": "3.379.1",
    "@workspace/env": "file:env",
    "@workspace/locators": "file:locators"
  },
  "devDependencies": {
    "@workspace/eslint-config": "file:eslint-config",
    "@workspace/tsconfig": "file:tsconfig"
  },
  "eslintConfig": {
    "extends": [
      "@workspace/eslint-config"
    ]
  }
}
