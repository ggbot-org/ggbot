{
  "name": "@ggbot2/devops",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/",
    "task:create": "node ./dist/task-create.js",
    "task:showDevopsPolicy": "node dist/task-showDevopsPolicy.js",
    "task:status": "node ./dist/task-status.js"
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/tsconfig": "file:tsconfig"
  },
  "dependencies": {
    "@ggbot2/aws": "file:aws",
    "@ggbot2/infrastructure": "file:infrastructure"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ]
  }
}
