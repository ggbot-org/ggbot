{
  "name": "@workspace/devops",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "task:create": "node ./dist/task-create.js",
    "task:showDevopsPolicy": "node dist/task-showDevopsPolicy.js",
    "task:status": "node ./dist/task-status.js"
  },
  "dependencies": {
    "@workspace/aws": "0.0.0",
    "@workspace/infrastructure": "0.0.0"
  },
  "devDependencies": {
    "@workspace/eslint-config": "0.0.0",
    "@workspace/tsconfig": "0.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@workspace/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "no-warning-comments": "off"
        }
      }
    ]
  }
}
