    "task:showDevopsPolicy": "node src/task-showDevopsPolicy.js",
    "task:status": "node ./dist/task-status.js",
    "tsc--noEmit": "tsc --noEmit --project ."
  },
  "dependencies": {
    "@ggbot2/aws": "file:aws"
  }
}
