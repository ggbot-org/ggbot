{
  "name": "@ggbot2/devops",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "cleanup": "rm -rf dist/",
    "task:create": "node ./dist/task-create.js",
    "task:showDevopsPolicy": "node src/task-showDevopsPolicy.js",
    "task:status": "node ./dist/task-status.js",
    "test": "npm run tsc--noEmit",
    "tsc--noEmit": "tsc --noEmit --project ."
  },
  "dependencies": {
    "@ggbot2/aws": "file:aws"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "typescript": "^4.7.4"
  }
}
