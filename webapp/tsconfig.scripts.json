{
	"compilerOptions": {
		"outDir": "./temp",
		"paths": {
			"_/routing/*": ["./src/routing/*"]
		},
		"rootDir": "./src",
		"tsBuildInfoFile": "temp/tsconfig.scripts.tsbuildinfo"
	},
	"extends": "@workspace/tsconfig",
	"include": [
		"src/dirnames.ts",
		"src/ecmaScripts.ts",
		"src/esbuild/*.ts",
		"src/html.ts",
		"src/i18n/FormatjsIntlMessageIds.ts",
		"src/i18n/locales.ts",
		"src/package.ts",
		"src/react/root.ts",
		"src/routing/admin/pages.ts",
		"src/routing/design/pages.ts",
		"src/routing/user/pages.ts",
		"src/routing/user/types.ts",
		"src/scripts/*.ts"
	]
}
