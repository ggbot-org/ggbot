{
  "name": "@ggbot2/aws-lambda-utrust-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node build.mjs",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ *.zip",
    "lint": "eslint --ext .ts .",
    "prebuild": "npm run cleanup",
    "prezip": "npm run build",
    "zip": "cd dist; zip -X -r ../build.zip * > /dev/null; cd .."
  },
  "dependencies": {
    "@ggbot2/api": "file:api",
    "@ggbot2/aws-lambda": "file:aws-lambda",
    "@ggbot2/http-status-codes": "file:http-status-codes"
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/tsconfig": "file:tsconfig"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ]
  }
}
