{
  "name": "@ggbot2/utrust",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run bundle && npm run zip",
    "bundle": "node scripts/bundle.js",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "create": "sh scripts/create.sh",
    "deploy": "sh scripts/deploy.sh",
    "lint": "eslint --ext .ts .",
    "prebuild": "npm run cleanup",
    "zip": "sh scripts/zip.sh"
  },
  "dependencies": {
    "@ggbot2/database": "file:database",
    "@ggbot2/env": "file:env",
    "@ggbot2/http-status-codes": "file:http-status-codes",
    "@ggbot2/locators": "file:locators",
    "@ggbot2/models": "file:models",
    "@utrustdev/utrust-ts-library": "^1.0.4",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/tsconfig": "file:tsconfig",
    "@types/aws-lambda": "^8.10.114",
    "esbuild": "^0.17.17"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ],
    "ignorePatterns": [
      "dist/",
      "temp/"
    ]
  }
}
