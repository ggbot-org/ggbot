{
  "name": "@ggbot2/web-storage",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts src/",
    "ts-prune": "ts-prune --error --ignore 'dist|temp|src/index\\.ts'"
  },
  "dependencies": {
    "@ggbot2/binance": "file:binance",
    "@ggbot2/cache": "file:cache",
    "@ggbot2/dflow": "file:dflow",
    "@ggbot2/env": "file:env",
    "@ggbot2/type-utils": "file:type-utils"
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/tsconfig": "file:tsconfig",
    "ts-prune": "0.10.3"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ]
  }
}
