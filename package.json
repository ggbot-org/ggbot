{
  "name": "ggbot2",
  "description": "crypto flow",
  "homepage": "http://ggbot2.com",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "author": "Gianluca Casati (http://fibo.github.io/)",
  "scripts": {
    "admin-webapp": "npm run dev --workspace admin-webapp",
    "build:admin-webapp": "rm -rf admin-webapp/.next; npm run build --workspace admin-webapp",
    "build:assets": "npm run build --workspace assets",
    "build:aws": "npm run build --workspace aws",
    "build:binance": "npm run build --workspace binance",
    "build:cookies": "npm run build --workspace cookies",
    "build:database": "npm run build --workspace database",
    "build:devops": "npm run build --workspace devops",
    "build:dflow": "npm run build --workspace dflow",
    "build:email-messages": "npm run build --workspace email-messages",
    "build:env": "npm run build --workspace env",
    "build:http-status-codes": "npm run build --workspace http-status-codes",
    "build:infrastructure": "npm run build --workspace infrastructure",
    "build:models": "npm run build --workspace models",
    "build:ui-components": "npm run build --workspace ui-components",
    "build:time": "npm run build --workspace time",
    "build:user-webapp": "npm run build:webapp --workspace user-webapp",
    "build:user-webapp:package": "npm run build:package --workspace user-webapp",
    "cleanup:.next": "rm -rf */.next",
    "cleanup:node_modules": "rm -rf */node_modules",
    "cleanup:dist": "rm -rf */dist",
    "e2e": "npm run playwright --workspace end-to-end-tests",
    "e2e:chromium": "npm run chromium --workspace end-to-end-tests",
    "e2e:show-report": "npm run show-report --workspace end-to-end-tests",
    "lint": "npm run lint --workspaces --if-present",
    "poste2e": "npm run e2e:show-report",
    "prebuild:admin-webapp": "npm run build:database; npm run build:http-status-codes; npm run build:ui-components",
    "prebuild:aws": "npm run build:infrastructure",
    "prebuild:assets": "npm run build:infrastructure",
    "prebuild:binance": "npm run build:dflow",
    "prebuild:database": "npm run build:aws; npm run build:models",
    "prebuild:devops": "npm run build:aws",
    "prebuild:cookies": "npm run build:models",
    "prebuild:dflow": "npm run build:models",
    "prebuild:infrastructure": "npm run build:env",
    "prebuild:models": "npm run build:time",
    "prebuild:ui-components": "npm run build:time",
    "prebuild:user-webapp": "npm run build:assets; npm run build:cookies; npm run build:database; npm run build:email-messages; npm run build:http-status-codes; npm run build:ui-components",
    "task:create": "npm run task:create --workspace devops",
    "task:showDevopsPolicy": "npm run task:showDevopsPolicy --workspace devops",
    "task:status": "npm run task:status --workspace devops",
    "test": "npm run test --workspaces --if-present",
    "test:admin-webapp": "npm run test --workspace admin-webapp",
    "test:aws": "npm run test --workspace aws",
    "test:binance": "npm run test --workspace binance",
    "test:cookies": "npm run test --workspace cookies",
    "test:database": "npm run test --workspace database",
    "test:devops": "npm run test --workspace devops",
    "test:dflow": "npm run test --workspace dflow",
    "test:env": "npm run test --workspace env",
    "test:http-status-codes": "npm run test --workspace http-status-codes",
    "test:infrastructure": "npm run test --workspace infrastructure",
    "test:models": "npm run test --workspace models",
    "test:ui-components": "npm run test --workspace ui-components",
    "test:time": "npm run test --workspace time",
    "test:user-webapp": "npm run test --workspace user-webapp",
    "tsc--noEmit": "npm run tsc--noEmit --workspaces --if-present",
    "ui-components-webapp": "npm run dev --workspace ui-components",
    "user-webapp": "npm run dev --workspace user-webapp"
  },
  "workspaces": [
    "admin-webapp",
    "assets",
    "aws",
    "binance",
    "cookies",
    "database",
    "devops",
    "dflow",
    "email-messages",
    "end-to-end-tests",
    "env",
    "http-status-codes",
    "infrastructure",
    "models",
    "time",
    "ui-components",
    "user-webapp"
  ],
  "keywords": [],
  "devDependencies": {
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13"
  }
}
