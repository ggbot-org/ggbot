    "eslint-config",
    "executor",
    "html",
    "http",
    "infrastructure",
    "locators",
    "models",
    "public-api",
    "react",
    "repo",
    "status",
    "test-data",
    "time",
    "tsconfig",
    "type-utils",
    "use-action",
    "user-api",
    "utrust-api",
    "www"
  ]
