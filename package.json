  "devDependencies": {
    "@types/node": "^18.15.6",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-sh": "^0.12.8",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "*.{cjs,js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,json,md,scss,sh}": [
      "prettier --write"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "check_types",
    "test"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-jsdoc",
      "prettier-plugin-sh"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "options": {
          "tsdoc": true
        }
      }
    ]
  },
  "workspaces": [
    "admin",
    "admin-api",
    "api",
    "api-gateway",
    "app",
    "arithmetic",
    "assets",
    "auth-api",
    "aws",
    "binance",
