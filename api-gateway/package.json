{
  "name": "@ggbot2/api-gateway",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --fix --ext .ts ."
  },
  "dependencies": {
    "@ggbot2/api": "file:api",
    "@ggbot2/env": "file:env",
    "@ggbot2/http": "file:http",
    "@ggbot2/locators": "file:locators",
    "@types/aws-lambda": "^8.10.114",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.17.17"
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/tsconfig": "file:tsconfig"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ],
    "ignorePatterns": [
      "dist/",
      "temp/"
    ]
  }
}
