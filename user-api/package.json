{
  "name": "@ggbot2/user-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run tsc && npm run bundle_lambda && npm run zip",
    "bundle_lambda": "node scripts/bundle_lambda.js",
    "check_types": "tsc --noEmit --project .",
    "cleanup": "rm -rf dist/ temp/",
    "lint": "eslint --ext .ts .",
    "prebuild": "npm run cleanup",
    "tsc": "tsc --build tsconfig.build.json",
    "zip": "sh scripts/zip.sh"
  },
  "dependencies": {
    "@ggbot2/api": "file:api",
    "@ggbot2/api-action": "file:api-action",
    "@ggbot2/http-status-codes": "file:http-status-codes",
    "@ggbot2/models": "file:models",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@ggbot2/eslint-config": "file:eslint-config",
    "@ggbot2/tsconfig": "file:tsconfig",
    "@types/aws-lambda": "^8.10.114",
    "esbuild": "^0.17.17"
  },
  "eslintConfig": {
    "extends": [
      "@ggbot2/eslint-config"
    ],
    "ignorePatterns": [
      "dist/",
      "temp/"
    ]
  }
}
